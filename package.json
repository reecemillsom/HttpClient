{
  "name": "http-request-client",
  "version": "2.0.0",
  "description": "Http Client wrapper for doing requests",
  "main": "./dist/index.js",
  "scripts": {
    "tsc": "tsc",
    "tslint": "rm -rf dist/ && tslint -c tslint.json src/**/*.ts",
    "test": "npm run tslint && npm run tsc && nyc mocha"
  },
  "author": "Reece Millsom",
  "license": "ISC",
  "keywords": [
    "http",
    "request",
    "httpclient",
    "get",
    "put",
    "post",
    "delete",
    "fetch"
  ],
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14.107",
    "@types/mocha": "^2.2.48",
    "@types/node-cache": "^4.1.1",
    "@types/sinon": "^4.3.0",
    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.1.2",
    "mocha": "^5.0.1",
    "nyc": "^11.6.0",
    "sinon": "^4.4.2",
    "sinon-chai": "^2.14.0",
    "source-map-support": "^0.5.4",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "lodash": "^4.17.5",
    "node-cache": "^4.2.0"
  },
  "nyc": {
    "lines": 95,
    "statements": 95,
    "functions": 90,
    "branches": 90,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "check-coverage": true,
    "sourceMap": true,
    "instrument": true,
    "report-dir": "./coverage"
  }
}
